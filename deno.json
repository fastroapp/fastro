{
  "lock": false,
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "https://esm.sh/react"
  },
  "tasks": {
    "hello-string": "deno run -A --unstable examples/hello_string.ts",
    "hello-jsx": "deno run -A --unstable examples/hello_jsx.tsx",
    "hello-json": "deno run -A --unstable examples/hello_json.ts",
    "hello": "deno run -A --unstable examples/hello_world.ts",
    "static": "deno run -A --unstable examples/static_file.ts",
    "ssr": "deno run -A --unstable examples/ssr_server.ts",
    "middleware": "deno run -A --unstable examples/middleware.ts",
    "jsx": "deno run -A --unstable examples/hello_world_fastro_jsx.tsx",
    "params": "deno run -A --unstable examples/params.ts"
  }
}
